        height: auto;
    }

}

View 4
@media screen and (max-width: 1024px) {

    img,
    .minion {
        background-color: rgb(139, 89, 89); <--
        height: 35%;
        width: 35%;
    }

    h2,
    .card {
        background-color: rgb(139, 89, 89); <--
        font-size: 25px;
        width: 350px;
        height: 300px;
    }

    img,
    .mb {
        background-color: rgb(139, 89, 89); <--
        height: 20%;
        width: 20%;
    }

    footer {
        background-color: rgb(139, 89, 89); <--
        height: 50%;
        width: 100%;
    }
    p,
    .display {
        background-color: rgb(139, 89, 89); <--
        text-align: center;
        font-size: 35px;
        width: auto;
        height: auto;
    }

}

View 5
@media screen and (max-width: 768px) {

    img,
    .minion {
        background-color: rgb(41, 118, 165); <--
        font-size: 25px;
        width: 450px;
        height: 450px;
    }

    h2,
    .card {
        background-color: rgb(41, 118, 165); <--
        font-size: 30px;
        height: 300px;
        width: 800px;
    }

    img,
    .mb {
        background-color: rgb(41, 118, 165); <--
        height:450px;
        width: 300px;
    }

    footer {
        background-color: rgb(41, 118, 165); <--
        margin: 5px;
        width: 100%;
    }
    p,
    .display {
        background-color: rgb(41, 118, 165); <--
        text-align: center;
        font-size: 35px;
        width: auto;
        height: auto;
    }
}

View 6
@media screen and (max-width: 425px) {

    h2,
    .card {
        background-color: rgb(165, 41, 128); <--
        font-size: 25px;
        width: 100%;
        height: 300px;
        align-items: center;
    }

    img,
    .minion {
        background-color: rgb(165, 41, 128); <--
        align-items: center;
        height: 100%;
        width: 100%;
    }

    img,
    .mb {
        background-color: rgb(165, 41, 128); <--
        width: 100%;
    }

    footer {
        background-color: rgb(165, 41, 128); <--
        width: 100%;
    }
    p,
    .display {
        background-color: rgb(165, 41, 128); <--
        text-align: center;
        font-size: 35px;
        width: auto;
        height: auto;
    }

}

